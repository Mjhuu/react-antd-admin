.content
  height: 100%
  background: #fff
  overflow-y auto
  margin: 1rem
  box-sizing border-box
  .tabpane-box
    position relative
    height: 0
  >.content-box
    position relative
    padding 1.3rem
    width: 100%
    height calc(100% - 40px)
    overflow: auto;
    >.content-inner
      width: 100%
      min-width 932px
      overflow-y auto
      overflow-x auto
.bg-box
  height: 200%
  background: #a6f3ff
  margin: 1rem
  box-sizing border-box